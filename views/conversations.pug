html
  head
    title= title
    link(rel="stylesheet", href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css", integrity="sha384-WskhaSGFgHYWDcbwN70/dfYBj47jz9qbsMId/iRN3ewGhXQFZCSftd1LZCfmhktB", crossorigin="anonymous")

  body
    main(role="main" class="container")
      h2 #{title.name} 
      h3 #{conversations.length}
      
      table.table
        tr
          th User
          th Tags
        each index in conversations
          tr
            td 
              a(href="https://app.intercom.io/a/apps/mbz4oxgz/inbox/inbox/1894039/conversations/"+index.id) 
                code #{index.id}
            td #{JSON.stringify(index.tags)}
      a(href="/") back